{
  "$schema": "./node_modules/@biomejs/biome/configuration_schema.json",
  "files": {
    "ignore": ["node_modules"]
  },
  "formatter": {
    "indentStyle": "space"
  },
  "linter": {
    "rules": {
      "style": {
        "recommended": false,
        "all": false
      },
      "suspicious": {
        "noExplicitAny": "off"
      }
    }
  }
}
